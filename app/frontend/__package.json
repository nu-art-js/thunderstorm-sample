{
  "name": "app-frontend",
  "version": "$APP_VERSION",
  "description": "Frontend hello world with Thunder",
  "keywords": [
    "TacB0sS",
    "boilerplate",
    "frontend",
    "infra",
    "nu-art",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:nu-art-js/thunderstorm-template.git"
  },
  "license": "Apache-2.0",
  "author": "TacB0sS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "export NODE_OPTIONS='--import data:text/javascript,import%20%7B%20register%20%7D%20from%20%22node%3Amodule%22%3B%20import%20%7B%20pathToFileURL%20%7D%20from%20%22node%3Aurl%22%3B%20register%28%22ts-node%2Fesm%22%2C%20pathToFileURL%28%22.%2F%22%29%29%3B' && webpack --mode development --config webpack.config.ts --env app.env=local",
    "start": "export NODE_OPTIONS='--import data:text/javascript,import%20%7B%20register%20%7D%20from%20%22node%3Amodule%22%3B%20import%20%7B%20pathToFileURL%20%7D%20from%20%22node%3Aurl%22%3B%20register%28%22ts-node%2Fesm%22%2C%20pathToFileURL%28%22.%2F%22%29%29%3B' && webpack-dev-server --config webpack.config.ts --env app.env=local --open --mode development "
  },
  "dependencies": {
    "@app/core-shared": "$APP_VERSION",
    "@app/core-frontend": "$APP_VERSION",
    "@nu-art/ts-focused-object": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/ts-styles": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/firebase": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/permissions": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/thunderstorm": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/ts-common": "$THUNDERSTORM_DEP_VERSION",
    "@nu-art/user-account": "$THUNDERSTORM_DEP_VERSION",
    "file-loader": "^6.2.0",
    "moment": "$MOMENT_PKG_VERSION",
    "react": "$REACT_PKG_VERSION",
    "react-datepicker": "$REACT_DATE_PICKER_PKG_VERSION",
    "react-dom": "$REACT_DOM_PKG_VERSION",
    "react-router-dom": "$REACT_ROUTER_DOM_PKG_VERSION"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "stream-browserify": "^3.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "util": "^0.12.4",
    "@types/history": "^4.7.2",
    "@types/react-datepicker": "$REACT_DATE_PICKER_TYPES_VERSION",
    "@types/react": "$REACT_TYPES_VERSION",
    "@types/react-dom": "$REACT_DOM_TYPES_VERSION",
    "@types/react-router-dom": "$REACT_ROUTER_DOM_TYPES_VERSION",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "csstype": "^2.6.7",
    "firebase": "$FIREBASE_PKG_VERSION",
    "history": "^4.9.0",
    "html-beautify-webpack-plugin": "^1.0.5",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "sass": "^1.49.9",
    "qs": "^6.6.0",
    "sass-loader": "$SASS_LOADER_PKG_VERSION",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^8.2.0",
    "xml-formatter": "^2.6.0",
    "url-loader": "^4.1.1",
    "webpack": "$WEBPACK_PKG_VERSION",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-manifest-plugin": "^5.0.0",
    "write-file-webpack-plugin": "^4.5.0",
    "vm-browserify": "latest"
  },
  "unitConfig": {
    "hasSelfHotReload": true,
    "servingPort": 8000,
    "type": "firebase-hosting",
    "label": "Petit Fawn - Frontend",
    "envs": {
      "local": {
        "config": {
          "configUrl": "http://127.0.0.1:8124/_config/frontend/manager.json?ns=shopify-manager-tool-local-default-rtdb"
        },
        "projectId": "shopify-manager-tool-local"
      }
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "import": "./*.js"
    }
  }
}
